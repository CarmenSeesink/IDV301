<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="seeturtle.MigrationPage">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout VerticalOptions="Start">
            <ImageButton BackgroundColor="#fdbb11"
                         Source="arrow"
                         VerticalOptions="Start"
                         Margin="30, 35, 0, 20"
                         HorizontalOptions="Start"
                         WidthRequest="48"
                         HeightRequest="48"
                         CornerRadius="24"
                         Padding="18"
                         Clicked="BackTapped">
            </ImageButton>
            <Label Text="Migration"
                   TextColor="Black"
                   VerticalOptions="Start"
                   FontSize="22"
                   FontFamily="Avenir"
                   FontAttributes="Bold"
                   Margin="30, 0, 0, 30">
            </Label>
            </StackLayout>
         </StackLayout>
    </ContentPage.Content>
</ContentPage>
